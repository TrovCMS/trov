{
    "name": "trovcms/trov",
    "type": "project",
    "description": "A CMS start kit for websites, built on Filament and Laravel.",
    "homepage": "https://github.com/trovcms/trov",
    "support": {
        "issues": "https://github.com/trovcms/trov/issues",
        "source": "https://github.com/trovcms/trov"
    },
    "keywords": [
        "laravel",
        "filament",
        "trov",
        "cms",
        "trovcms"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "awcodes/filament-addons": "^0.3.12",
        "awcodes/filament-curator": "^0.4.4",
        "awcodes/filament-quick-create": "^1.0.4",
        "awcodes/filament-sticky-header": "^1.1.0",
        "awcodes/filament-tiptap-editor": "^0.3.12",
        "bezhansalleh/filament-shield": "^2.2.3",
        "filament/filament": "^2.15.45",
        "filament/spatie-laravel-tags-plugin": "^2.15.45",
        "guzzlehttp/guzzle": "^7.2",
        "hisorange/browser-detect": "^4.5",
        "jeffgreco13/filament-breezy": "^1.4.2",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "pxlrbt/filament-environment-indicator": "^0.2.0",
        "spatie/laravel-ray": "^1.30",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-tags": "^4.3.2",
        "trovcms/core": "^0.1.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}